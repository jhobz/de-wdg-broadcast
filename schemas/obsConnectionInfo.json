{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"address": {
			"type": "string"
		},
		"password": {
			"type": "string"
		},
		"reconnect": {
			"type": "boolean"
		}
	},
	"additionalProperties": false,
	"required": ["address", "password"],
	"default": {
		"address": "",
		"password": "",
		"reconnect": true
	}
}
